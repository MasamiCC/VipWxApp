<!--pages/index/alltype/alltype.wxml-->
<block>
  <view>
    <view class="alltype_list"  wx:for="{{hotType}}" wx:key="item" bindtap='_goType' data-typeid='{{item.id}}' data-type_name='{{item.name}}'>
      <view class='orangebackground'>
          <view class='typefont'>{{item.name}}</view>
          <view class='go_button'><image class='go_button_img' src='/statics/imgs/rightgo.png'></image></view>
      </view>
    </view>
  </view>
</block>
