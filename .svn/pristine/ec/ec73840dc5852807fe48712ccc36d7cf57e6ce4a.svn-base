/* pages/Components/not-login/not-login.wxss */

.login_nav{
  width: 100%;
  height: 100%;
  overflow: hidden;
  position: relative;
}

/*背景图片*/
image.bg{
  position: absolute;
  width: 200rpx;
  height: 260rpx;
}

.bg.bg_1{
  top: 40rpx;
  left: 0
}

.bg.bg_2{
  top: 500rpx;
  right: 0;
  width: 100rpx;
  height: 200rpx;
}

.login_area{
  width: 100%;
  height: 100%;
  display: flex;
  align-items: center;
  flex-direction: column;
  padding-top: 300rpx;
}

.login_logo{
  width: 200rpx;
  height: 150rpx;
  margin-bottom: 150rpx;
}

.btn_area{
  height: 70rpx;
  line-height: 70rpx;
  border-radius: 50rpx;
  border: 1px solid #999999;
  padding: 5rpx;
}

.login_btn{
  background-color: #333333;
  color: #ffffff;
  font-size: 28rpx;
  padding: 0 60rpx;
  border-radius: 50rpx;
}